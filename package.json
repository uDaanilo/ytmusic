{
  "name": "ytmusic",
  "version": "1.0.2",
  "description": "A youtube music app",
  "main": "dist/index.js",
  "author": "uDaanilo",
  "license": "MIT",
  "repository": "github:uDaanilo/ytmusic",
  "scripts": {
    "start": "tsc && cp -r src/static dist && electron dist",
    "dev": "tsc && cp -r src/static dist && electron dist NODE_ENV=development",
    "build:publish": "electron-builder --publish always",
    "build": "tsc && cp -r src/static dist && electron-builder build"
  },
  "devDependencies": {
    "@types/discord-rpc": "^4.0.0",
    "electron": "^25.0.0",
    "electron-builder": "^22.11.7",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.22.5",
    "cross-fetch": "^3.1.4",
    "discord-rpc": "^4.0.1"
  },
  "build": {
    "appId": "com.github.udaanilo.ytmusic",
    "productName": "YTMusic",
    "asar": true,
    "directories": {
      "output": "out"
    },
    "files": [
      "!src",
      "!.gitignore",
      "!README.md",
      "!tsconfig.json"
    ],
    "extraFiles": [
      "dist/static/config.json"
    ],
    "win": {
      "icon": "dist/static/img/icon.png"
    },
    "nsis": {
      "installerIcon": "dist/static/img/icon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "releaseType": "release"
      }
    ]
  }
}
